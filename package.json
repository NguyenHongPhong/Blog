{
    "name": "blog",
    "version": "1.0.0",
    "description": "This is Blog website using NodeJS",
    "main": "app.js",
    "type": "module",
    "scripts": {
        "start": "nodemon --inspect ./src/app.js",
        "watch": "node-sass --watch ./src/resources/scss --output ./src/resources/public/css",
        "prepare": "husky"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
