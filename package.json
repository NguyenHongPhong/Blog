        "express-handlebars": "^8.0.1",
        "mongoose": "^8.11.0",
        "nanoid": "^5.1.2"
    },
    "devDependencies": {
        "husky": "^9.1.7",
        "lint-staged": "^15.4.3",
        "morgan": "^1.10.0",
        "node-sass": "^9.0.0",
        "nodemon": "^3.1.9",
        "prettier": "3.5.2"
    }
}
